public class Player {
	String gender;
	boolean contingent;
	int priority;
	public void set_priority(String g, boolean c){
		if(g.equals("F")&& !c)
		{
			priority = 1;
			
		}
		if(g.equals("F")&& c)
		{
			priority = 2;
			
		}
		if(g.equals("M")&& c)
		{
			priority = 3;
			
		}
		if(g.equals("M")&& !c)
		{
			priority = 4;
			
		}
	
		
		
	}
	public int get_priority(){
		return priority;
		
	}

}
